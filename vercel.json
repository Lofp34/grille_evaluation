{
  "version": 2,
  "buildCommand": "echo 'Static HTML - no build required'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/grille",
      "destination": "/grille_express_application_html.html"
    },
    {
      "source": "/evaluation",
      "destination": "/grille_express_application_html.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
